{
  "name": "scotdance-challenge",
  "version": "0.16.0",
  "private": true,
  "scripts": {
    "emulate": "firebase emulators:start --import=.firebase/emulator-data",
    "dev:functions": "npm run --prefix functions/ dev",
    "dev:www": "vite --host",
    "dev": "run-p dev:functions \"emulate -- --export-on-exit\" dev:www",
    "lint": "eslint './{{functions,src}/**,}/*.{js,ts,vue}'",
    "lint:fix": "npm run lint -- --fix",
    "build:functions": "npm run --prefix functions/ build",
    "build:www": "vite build",
    "build": "npm run build:functions && npm run build:www",
    "report": "npm run build:www && open stats.html",
    "postinstall": "npm --prefix ./functions/ i"
  },
  "dependencies": {
    "animated-number-vue": "^1.0.0",
    "chart.js": "^2.9.4",
    "countries-list": "^2.5.4",
    "date-fns": "^2.12.0",
    "firebase": "^7.12.0",
    "lodash.get": "^4.4.2",
    "lodash.orderby": "^4.6.0",
    "runes": "^0.4.3",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-infinite-scroll": "^2.0.2",
    "vue-localstorage": "^0.6.2",
    "vue-router": "^3.1.5",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.2",
    "vuex-persist": "^2.2.0",
    "vuexfire": "^3.2.2"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.16",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/runes": "^0.4.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.3.0",
    "firebase-tools": "^13.31.1",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "~1.32",
    "typescript": "^4.5.5",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-vue2": "^2.0.3",
    "vue-template-compiler": "^2.6.14"
  }
}
